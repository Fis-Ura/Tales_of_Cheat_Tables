          <AssemblerScript>{$lua}
if syntaxcheck then return end

function mainloop()
   if not (activated) then
      activated = 0
   end

   local Slot1Chara = readSmallInteger("baseRAMAddr+1CE758")
   if (Slot1Chara == 0) or (Slot1Chara == nil) then
      return
   else
      Slot1Chara = Slot1Chara - 1
   end
   if not (Slot1Chara == nil) then
      --Battle Mode
      if (readByte("baseRAMAddr+1FFA44") == 1) then
      --Compare P1/Slot 1 In-Battle Max HP/TP to Max HP/TP Stat
      --if (readSmallInteger("baseRAMAddr+1E5FD4") == readSmallInteger("baseRAMAddr+1CE0CE+AC*"..Slot1Chara)) and
      --   (readSmallInteger("baseRAMAddr+1E5FDA") == readSmallInteger("baseRAMAddr+1CE0D4+AC*"..Slot1Chara)) then
      --This method doesn't work in this game as the data stays in memory
         if (activated == 0) then
            --Control Modes
            --Slot 1
            if (readByte("controlModemem") == 1) then
               writeByte("baseRAMAddr+171DC0",2)
            end
            --Slot 2
            if (readByte("controlModemem+1") == 1) then
               writeByte("baseRAMAddr+171DC1",2)
            end
            --Slot 3
            if (readByte("controlModemem+2") == 1) then
               writeByte("baseRAMAddr+171DC2",2)
            end
            --Slot 4
            if (readByte("controlModemem+3") == 1) then
               writeByte("baseRAMAddr+171DC3",2)
            end

            --Dash Modes
            --Slot 1
            if (readByte("controlModemem+4") == 1) then
               writeByte("baseRAMAddr+1E617F",1)
            end
            --Slot 2
            if (readByte("controlModemem+5") == 1) then
               writeByte("baseRAMAddr+1E617F+1D4",1)
            end
            --Slot 3
            if (readByte("controlModemem+6") == 1) then
               writeByte("baseRAMAddr+1E617F+1D4*2",1)
            end
            --Slot 4
            if (readByte("controlModemem+7") == 1) then
               writeByte("baseRAMAddr+1E617F+1D4*3",1)
            end

            --Combo Command Modes
            --Slot 1
            if (readByte("controlModemem+8") == 1) then
               writeByte("baseRAMAddr+1E6181",1)
            end
            --Slot 2
            if (readByte("controlModemem+9") == 1) then
               writeByte("baseRAMAddr+1E6182",1)
            end
            --Slot 3
            if (readByte("controlModemem+0xA") == 1) then
               writeByte("baseRAMAddr+1E6183",1)
            end
            --Slot 4
            if (readByte("controlModemem+0xB") == 1) then
               writeByte("baseRAMAddr+1E6184",1)
            end
            activated = 1
         end
      --Field Mode
      else
         activated = 0
      end

   end
end
{$asm}
[ENABLE]
alloc(controlModemem,1000)

controlModemem:
  db 01 00 00 00 01 01 01 01

controlModemem+C:
  db 01 01 01 01

registersymbol(controlModemem)

{$lua}
if syntaxcheck then return end
if not timer then
   timer = createTimer(nil)
   timer.OnTimer = function(timer)
                   mainloop()
                   end
   timer.Interval = 1000
   timer.Enabled = true
else
   timer.Enabled = true
end
{$asm}
[DISABLE]
{$lua}
if syntaxcheck then return end
   if (timer) then
      object_destroy(timer)
   end
   timer = nil
   activated = nil
{$asm}
dealloc(controlModeMem)
unregistersymbol(controlModemem)
