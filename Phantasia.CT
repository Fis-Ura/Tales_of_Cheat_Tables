          <Description>"Auto Control/Dash/Combo Command Modes + Auto Death Cam"</Description>
          <Options moHideChildren="1"/>
          <VariableType>Auto Assembler Script</VariableType>
          <AssemblerScript>{$lua}
if syntaxcheck then return end

function mainloop()
   if not (activated) then
      activated = 0
   end

   local Slot1Chara = readSmallInteger("baseRAMAddr+0873F8")
   if (Slot1Chara == 0) or (Slot1Chara == nil) then
      return
   else
      Slot1Chara = Slot1Chara - 1
   end
   if not (Slot1Chara == nil) then
      --Battle Mode
      --if (readByte("baseRAMAddr+13613D") == 1) then
      --Compare P1/Slot 1 In-Battle Max HP/TP to Max HP/TP Stat
      if (readSmallInteger("baseRAMAddr+138E24") == readSmallInteger("baseRAMAddr+086F0E+A0*"..Slot1Chara)) and
         (readSmallInteger("baseRAMAddr+138E30") == readSmallInteger("baseRAMAddr+086F14+A0*"..Slot1Chara)) then
         if (activated == 0) then
            --Control Modes
            --Slot 1
            if (readByte("controlModemem") == 1) then
               writeByte("baseRAMAddr+138F26",readByte("controlModemem+10"))
            end
            --Slot 2
            if (readByte("controlModemem+1") == 1) then
               writeByte("baseRAMAddr+138F26+238",readByte("controlModemem+11"))
            end
            --Slot 3
            if (readByte("controlModemem+2") == 1) then
               writeByte("baseRAMAddr+138F26+238*2",readByte("controlModemem+12"))
            end
            --Slot 4
            if (readByte("controlModemem+3") == 1) then
               writeByte("baseRAMAddr+138F26+238*3",readByte("controlModemem+13"))
            end

            --Dash Modes
            --Slot 1
            if (readByte("controlModemem+4") == 1) then
               writeByte("baseRAMAddr+138F1B",1)
            end
            --Slot 2
            if (readByte("controlModemem+5") == 1) then
               writeByte("baseRAMAddr+138F1B+238",1)
            end
            --Slot 3
            if (readByte("controlModemem+6") == 1) then
               writeByte("baseRAMAddr+138F1B+238*2",1)
            end
            --Slot 4
            if (readByte("controlModemem+7") == 1) then
               writeByte("baseRAMAddr+138F1B+238*3",1)
            end

            --Combo Command Modes
            --Slot 1
            if (readByte("controlModemem+8") == 1) then
               writeByte("baseRAMAddr+138F1D",1)
            end
            --Slot 2
            if (readByte("controlModemem+9") == 1) then
               writeByte("baseRAMAddr+138F1D+238",1)
            end
            --Slot 3
            if (readByte("controlModemem+0xA") == 1) then
               writeByte("baseRAMAddr+138F1D+238*2",1)
            end
            --Slot 4
            if (readByte("controlModemem+0xB") == 1) then
               writeByte("baseRAMAddr+138F1D+238*3",1)
            end
            activated = 1
         end
         --Auto Death Cam
         if not (autoCam1active) then
            autoCam1active = 0
            autoCam2active = 0
            autoCam3active = 0
            autoCam4active = 0
         end

         --If Auto Death Cam is enabled, check if character is dead
         --If dead, check if it has been ran, if not, store current control to
         --memory then set to Auto
         --Else, restore control mode from memory

         --Slot 1
         if (readByte("controlModemem+C") == 1) then
            if (readByte("baseRAMAddr+138F16") == 1) then --Character State (0 = Alive, 1 = Dead)
               if (autoCam1active == 0) then
                  writeByte("controlModemem+0x10",readByte("baseRAMAddr+138F26")) --Store Control Mode to temp memory
                  writeByte("baseRAMAddr+138F26",0)
                  autoCam1active = 1
               end
            else
               if (autoCam1active == 1) then
                  writeByte("baseRAMAddr+138F26",readByte("controlModemem+0x10"))
                  autoCam1active = 0
               end
            end
         end
         --Slot 2
         if (readByte("controlModemem+D") == 1) then
            if (readByte("baseRAMAddr+138F16+238") == 1) then --Character State (0 = Alive, 1 = Dead)
               if (autoCam2active == 0) then
                  writeByte("controlModemem+0x11",readByte("baseRAMAddr+138F26+238")) --Store Control Mode to temp memory
                  writeByte("baseRAMAddr+138F26+238",0)
                  autoCam2active = 1
               end
            else
               if (autoCam2active == 1) then
                  writeByte("baseRAMAddr+138F26+238",readByte("controlModemem+0x11"))
                  autoCam2active = 0
               end
            end
         end
         --Slot 3
         if (readByte("controlModemem+E") == 1) then
            if (readByte("baseRAMAddr+138F16+238*2") == 1) then --Character State (0 = Alive, 1 = Dead)
               if (autoCam3active == 0) then
                  writeByte("controlModemem+0x12",readByte("baseRAMAddr+138F26+238*2")) --Store Control Mode to temp memory
                  writeByte("baseRAMAddr+138F26+238*2",0)
                  autoCam3active = 1
               end
            else
               if (autoCam3active == 1) then
                  writeByte("baseRAMAddr+138F26+238*2",readByte("controlModemem+0x12"))
                  autoCam3active = 0
               end
            end
         end
         --Slot 4
         if (readByte("controlModemem+F") == 1) then
            if (readByte("baseRAMAddr+138F16+238*3") == 1) then --Character State (0 = Alive, 1 = Dead)
               if (autoCam4active == 0) then
                  writeByte("controlModemem+0x13",readByte("baseRAMAddr+138F26+238*3")) --Store Control Mode to temp memory
                  writeByte("baseRAMAddr+138F26+238*3",0)
                  autoCam4active = 1
               end
            else
               if (autoCam4active == 1) then
                  writeByte("baseRAMAddr+138F26+238*3",readByte("controlModemem+0x13"))
                  autoCam4active = 0
               end
            end
         end
      --Field Mode
      else
         activated = 0
      end

   end
end
{$asm}
[ENABLE]
alloc(controlModemem,1000)

controlModemem:
  db 01 00 00 00 01 01 01 01

controlModemem+C:
  db 01 01 01 01 02 02 02 02

registersymbol(controlModemem)

{$lua}
if syntaxcheck then return end
if not timer then
   timer = createTimer(nil)
   timer.OnTimer = function(timer)
                   mainloop()
                   end
   timer.Interval = 1000
   timer.Enabled = true
else
   timer.Enabled = true
end
{$asm}
[DISABLE]
{$lua}
if syntaxcheck then return end
   if (timer) then
      object_destroy(timer)
   end
   timer = nil
   activated = nil
   autoCam1Active = nil
   autoCam2Active = nil
   autoCam3Active = nil
   autoCam4Active = nil
{$asm}
dealloc(controlModeMem)
unregistersymbol(controlModemem)
