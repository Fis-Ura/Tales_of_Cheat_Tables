         if (readByte("controlModemem+E") == 1) then
            if (readByte("baseRAMAddr+138F16+238*2") == 1) then --Character State (0 = Alive, 1 = Dead)
               if (autoCam3active == 0) then
                  writeByte("controlModemem+0x12",readByte("baseRAMAddr+138F26+238*2")) --Store Control Mode to temp memory
                  writeByte("baseRAMAddr+138F26+238*2",0)
                  autoCam3active = 1
