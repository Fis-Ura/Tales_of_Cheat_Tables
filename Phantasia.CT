      <AssemblerScript>{$lua}
function fallbackScan()
   print("Using fallback... this may take a few seconds")
   r,error=autoAssemble([[aobscan(baseRAMAddr,? ? ? ? 80 0C 5A 27 08 00 40 03 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 02 00 00 00 00 00 00 00 FF 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00) // should be unique
   registersymbol(baseRAMAddr)]])
   if (r==false) then
     error(print(error .. "\nMake sure the game is running!"))
   end
   return
end
{$asm}
[ENABLE]
{$lua}
if syntaxcheck then return end
if not (process == nil) then
  if (string.lower(process):match("duckstation")) then
    if not (getAddressSafe("RAM") == nil) and not (getAddressSafe("RAM") == 0) then
       --For some reason, the above statement is true even if it is actually 0
       if not (readPointer("RAM") == nil) and not (readPointer("RAM") == 0) then
          registerSymbol("baseRAMAddr",readPointer(getAddress("RAM")))
          return
       else
          fallbackScan()
       end
    else
       fallbackScan()
    end
  else
      fallbackScan()
  end
else
  error(print("Not attached to anything"))
  return
end
{$asm}
[DISABLE]
unregistersymbol(baseRAMAddr)
