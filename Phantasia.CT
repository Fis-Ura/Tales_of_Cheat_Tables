                  <VariableType>Auto Assembler Script</VariableType>
                  <AssemblerScript>/*
You may experience crashes and/or unintended behaviour when using
this script. You have been warned.

This is mainly for quality of life purposes in multiplayer settings.

These scripts let you enable Dash/Combo Command Modes and configure your
Control Modes while in a battle.

You will not be able to use the Control/Dash Modes and Combo Command Modes
part of the table after activating one of the scripts.
However, you can edit them directly under the script.

Use the Toggles to control what gets enabled.

Auto Death Cam automatically changes Control Modes to Auto on death,
then returns to original Control Mode after getting brought back to life.
Can be configured on a per-player basis.
Don't be on Auto if you're not dead, otherwise it will break.
In case you did not heed this warning, if using Automatic Control Modes,
you can set the current Control Method in the "Active" section
under "Control Modes" (or you can just press Select),
otherwise set it under the "Automatic Control Enabler" section.
*/
[ENABLE]
[DISABLE]
</AssemblerScript>
                </CheatEntry>
                <CheatEntry>
                  <ID>63106</ID>
                  <Description>"Automatic Control Enabler"</Description>
                  <Options moHideChildren="1" moDeactivateChildrenAsWell="1"/>
                  <VariableType>Auto Assembler Script</VariableType>
                  <AssemblerScript>[ENABLE]

aobscanmodule(autoManual,mednafen_psx_hw_libretro.dll,41 88 10 0F B6 D2) // should be unique
alloc(newmem,$1000,"mednafen_psx_hw_libretro.dll"+10D74)

label(code)
label(partyCress)
label(partyMint)
label(partyArche)
label(partyClaus)
label(partyChester)
label(partySuzu)
label(controlEnabled)
label(dashEnabled)
label(comboEnabled)
label(controlSlot1)
label(controlSlot2)
label(controlSlot3)
label(controlSlot4)
label(dashSlot1)
label(dashSlot2)
label(dashSlot3)
label(dashSlot4)
label(comboSlot1)
label(comboSlot2)
label(comboSlot3)
label(comboSlot4)
label(activate)
label(controlActivate)
label(dashActivate)
label(comboActivate)
label(backtoGame)
label(return)

newmem:

//Check who is in the first slot is, and then,
//moves to their respective party blocks.
code:
  push eax
  cmp word [400873F8],1
  je partyCress
  cmp word [400873F8],2
  je partyMint
  cmp word [400873F8],3
  je partyArche
  cmp word [400873F8],4
  je partyClaus
  cmp word [400873F8],5
  je partyChester
  cmp word [400873F8],6
  je partySuzu
  jmp backtoGame

//Compare Character Max HP/TP to Slot 1 Max HP/TP, if same, go to activate
//Basically, just a way to check if you are in battle,
//as setting it all the time would break the game.
//An ugly hack, but it works nonetheless.
partyCress:
  movzx eax, word [40086F0E] //Character Max HP
  cmp eax,[40138E24] //Slot 1 Max HP
  je activate
  movzx eax, word [40086F14] //Character Max TP
  cmp eax,[40138E30] //Slot 1 Max TP
  je activate
  jmp backtoGame

partyMint:
  movzx eax, word [40086FAE] //Character Max HP
  cmp eax,[40138E24] //Slot 1 Max HP
  je activate
  movzx eax, word [40086FB4] //Character Max TP
  cmp eax,[40138E30] //Slot 1 Max TP
  je activate
  jmp backtoGame

partyArche:
  movzx eax, word [4008704E] //Character Max HP
  cmp eax,[40138E24] //Slot 1 Max HP
  je activate
  movzx eax, word [40087054] //Character Max TP
  cmp eax,[40138E30] //Slot 1 Max TP
  je activate
  jmp backtoGame

partyClaus:
  movzx eax, word [400870EE] //Character Max HP
  cmp eax,[40138E24] //Slot 1 Max HP
  je activate
  movzx eax, word [400870F4] //Character Max TP
  cmp eax,[40138E30] //Slot 1 Max TP
  je activate
  jmp backtoGame

partyChester:
  movzx eax, word [4008718E] //Character Max HP
  cmp eax,[40138E24] //Slot 1 Max HP
  je activate
  movzx eax, word [40087194] //Character Max TP
  cmp eax,[40138E30] //Slot 1 Max TP
  je activate
  jmp backtoGame

partySuzu:
  movzx eax, word [4008722E] //Character Max HP
  cmp eax,[40138E24] //Slot 1 Max HP
  je activate
  movzx eax, word [40087234] //Character Max TP
  cmp eax,[40138E30] //Slot 1 Max TP
  je activate
  jmp backtoGame

activate:
  cmp byte [controlEnabled],1
  je controlActivate
  cmp byte [dashEnabled],1
  je dashActivate
  cmp byte [comboEnabled],1
  je comboActivate
  jmp backtoGame

//This is where you control what values get set in battles.
controlActivate:
  mov ah, [controlSlot1]
  mov byte [40138F26],ah //Slot 1 Control Mode
  mov ah, [controlSlot2]
  mov byte [4013915E],ah //Slot 2 Control Mode
  mov ah, [controlSlot3]
  mov byte [40139396],ah //Slot 3 Control Mode
  mov ah, [controlSlot4]
  mov byte [401395CE],ah //Slot 4 Control Mode
  cmp [dashEnabled],1
  je dashActivate
  cmp [comboEnabled],1
  je comboActivate
  jmp backtoGame

dashActivate:
  mov ax, [dashSlot1]
  mov word [40138F1B],ax //Slot 1 Dash Mode
  mov ax, [dashSlot2]
  mov word [40139153],ax //Slot 2 Dash Mode
  mov ax, [dashSlot3]
  mov word [4013938B],ax //Slot 3 Dash Mode
  mov ax, [dashSlot4]
  mov word [401395C3],ax //Slot 4 Dash Mode
  mov ax, [comboSlot1]
  cmp [comboEnabled],1
  je comboActivate
  jmp backtoGame

comboActivate:
  mov word [40138F1D],ax //Slot 1 Combo Command Mode
  mov ax, [comboSlot2]
  mov word [40139155],ax //Slot 2 Combo Command Mode
  mov ax, [comboSlot3]
  mov word [4013938D],ax //Slot 3 Combo Command Mode
  mov ax, [comboSlot4]
  mov word [401395C5],ax //Slot 4 Combo Command Mode
  jmp backtoGame

backtoGame:
  pop eax
  mov [r8],dl
  movzx edx,dl
  jmp return

controlEnabled:
 dd 1

dashEnabled:
 dd 1

comboEnabled:
 dd 1

controlSlot1:
 dd 2

controlSlot2:
 dd 0

controlSlot3:
 dd 0

controlSlot4:
 dd 0

dashSlot1:
 dd 1

dashSlot2:
 dd 1

dashSlot3:
 dd 1

dashSlot4:
 dd 1

comboSlot1:
 dd 0

comboSlot2:
 dd 0

comboSlot3:
 dd 0

comboSlot4:
 dd 0

autoManual:
  jmp newmem
  nop

return:
registersymbol(autoManual)
registersymbol(controlEnabled)
registersymbol(dashEnabled)
registersymbol(comboEnabled)
registersymbol(controlSlot1)
registersymbol(controlSlot2)
registersymbol(controlSlot3)
registersymbol(controlSlot4)
registersymbol(dashSlot1)
registersymbol(dashSlot2)
registersymbol(dashSlot3)
registersymbol(dashSlot4)
registersymbol(comboSlot1)
registersymbol(comboSlot2)
registersymbol(comboSlot3)
registersymbol(comboSlot4)


[DISABLE]

autoManual:
  db 41 88 10 0F B6 D2

unregistersymbol(autoManual)
unregistersymbol(controlEnabled)
unregistersymbol(dashEnabled)
unregistersymbol(comboEnabled)
unregistersymbol(controlSlot1)
unregistersymbol(controlSlot2)
unregistersymbol(controlSlot3)
unregistersymbol(controlSlot4)
unregistersymbol(dashSlot1)
unregistersymbol(dashSlot2)
unregistersymbol(dashSlot3)
unregistersymbol(dashSlot4)
unregistersymbol(comboSlot1)
unregistersymbol(comboSlot2)
unregistersymbol(comboSlot3)
unregistersymbol(comboSlot4)
dealloc(newmem)
</AssemblerScript>
